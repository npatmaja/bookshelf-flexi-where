{
  "name": "bookshelf-flexi-where",
  "version": "0.1.3",
  "description": "Bookshelf plugin for more flexible where statement",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src/**/*.js",
    "test": "mocha --compilers js:babel-core/register --bail src/**/*.spec.js",
    "test:coverage": "istanbul cover _mocha -- --compilers js:babel-core/register --bail src/**/*.spec.js",
    "prepublish": "npm run clean && npm run build",
    "clean": "rm -rf dist",
    "build": "babel -q -L -D ./src/ --out-dir ./dist/ --ignore ./src/*.spec.js"
  },
  "keywords": [
    "bookshelf",
    "where",
    "clause",
    "flexible"
  ],
  "author": "Nauval Atmaja <nauval.atmaja@gmail.com> (http://nauvalatmaja.com/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/npatmaja/bookshelf-flexi-where.git"
  },
  "bugs": {
    "url": "https://github.com/npatmaja/bookshelf-flexi-where/issues"
  },
  "babel": {
    "presets": [
      "es2015",
      "node5",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ],
    "env": {
      "test": {
        "plugins": [
          "rewire"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "glogbals": {
      "__DEV__": true
    },
    "env": {
      "mocha": true,
      "node": true
    },
    "rules": {
      "no-confusing-arrow": 0
    }
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-rewire": "^1.0.0-rc-5",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-node5": "^11.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "bluebird": "^3.4.1",
    "bookshelf": "^0.10.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "eslint": "^3.3.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.0",
    "istanbul": "^1.1.0-alpha.1",
    "knex": "^0.11.10",
    "mocha": "^3.0.2",
    "mocha-lcov-reporter": "^1.2.0",
    "sqlite3": "^3.1.4"
  },
  "dependencies": {
    "lodash": "^4.15.0"
  }
}
